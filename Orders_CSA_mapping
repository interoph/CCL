						;-----------------------------------
						,"Fentanyl"     ; <--- primary
						,"Duragesic"    ;
						;---------------------------
						,"Hydrocodone"			   ;  <-- Primary
						/*,"Dihydrocodeinone"*/    ;
						,"Coristine-DH"            ;
						,"Dimetane Expectorant-DC" ;
						,"Hycodan"                 ;
						,"Hycomine syrup"          ;
						;-----------------------------------
						,"Hydrocodone (Dihydrocodeinone)"  ; <--primary
						,"Hycomine-S (pediatric syrup)"
						,"Novahistex DH"
						,"Novahistex DH Expectorant"
						,"Novahistine DH"
						,"Ratio-Coristex-DH"
						,"Tussionex"
						;------------------------------------
						,"Hydromorphone"                    ; <--  primary
						/* Dihydromorphinone */             ;

						,"Butalbital"         ;  <--primary
						,"Fiorinal"           ;
						,"Fiorinal C 1/2"     ;
						,"Fiorinal C 1/4"     ;
						,"Ratio-Tecnal"       ;
						,"Ratio-Tecnal C 1/2" ;
						,"Ratio-Tecnal C 1/4" ;
						;----------------------
						,"Butorphanol"        ;  <--primary
						,"Stadol NS"          ;
						;----------------------
						/*
						Codeine when prescribed as a single entity or 
						when included in a preparation containing 60 mg or more per dosage unit
						*/
						;-----------------------------------

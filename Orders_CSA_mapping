select 
oc.primary_mnemonic
,oc.cki
,ocs.mnemonic
,ocs.cki



from 

order_catalog oc
,order_catalog_synonym ocs



plan oc
where oc.active_ind = 1


join ocs
where ocs.catalog_cd = oc.catalog_cd
and ocs.active_ind = 1
and ocs.mnemonic in (
						;----------------
						"Alfentanil"    ; <--primary?
